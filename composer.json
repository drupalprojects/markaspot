{
  "name": "drupal/markaspot",
  "description": "Installation profile of the Mark-a-Spot D8 Distribution",
  "type": "drupal-profile",
  "license": "GPL-2.0+",
  "homepage": "https://github.com/markaspot/markaspot.git",
  "authors": [
    {
      "name": "Holger Kreis (markaspot)",
      "homepage": "https://www.drupal.org/u/markaspot",
      "role": "Maintainer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "hubspot/pace",
        "version": "0.7.5",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/HubSpot/pace/archive/v0.7.5.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "vcs",
      "url": "https://github.com/mishal/GeoPHP"
    }
  ],
  "require": {
    "composer/installers": "^1.2",
    "drupal-composer/drupal-scaffold": "^2.2",
    "cweagans/composer-patches": "~1.0",
    "drush/drush": "~8.0",
    "markaspot/markaspot_open311": "*",
    "markaspot/markaspot_blocks": ">8@dev",
    "markaspot/markaspot_uuid": ">8@dev",
    "markaspot/markaspot_ui": ">8@dev",
    "markaspot/markaspot_static_json": ">8@dev",
    "markaspot/markaspot_map": ">8@dev",
    "markaspot/markaspot_features": ">8@dev",
    "markaspot/markaspot_validation": ">8@dev",
    "markaspot/markaspot_token": ">8@dev",
    "markaspot/markaspot_default_content": ">8@dev",
    "markaspot/service_request": ">8@dev",
    "markaspot/masradix": ">8@dev",
    "drupal/address": "~1.0",
    "drupal/features": "^3.0",
    "drupal/core": "8.4.*",
    "drupal/radix": "3.*",
    "drupal/pathauto": "^1.0@RC",
    "drupal/admin_toolbar": "1.*",
    "drupal/restui": "^1.15",
    "drupal/token": "1.*",
    "drupal/shariff": ">=1.1",
    "drupal/r4032login": "dev-1.x",
    "drupal/views_bootstrap": "^3.0",
    "drupal/better_normalizers": "1.x-dev",
    "drupal/ctools": "3.x-dev",
    "drupal/image_effects": "1.x-dev",
    "drupal/paragraphs": "^1.2",
    "drupal/entity_reference_revisions": "^1.3",
    "drupal/diff": "^1.0",
    "drupal/field_group": "^1.0",
    "drupal/form_mode_manager": "^1.2",
    "drupal/geolocation": "^1.11",
    "drupal/rules": "3.x-dev",
    "drupal/maillog": "1.x-dev",
    "drupal/fa_icon_class": "1.*",
    "drupal/services_api_key_auth": "1.*",
    "bower-asset/pace": "0.7.6",
    "npm-asset/bootstrap-fileinput":"^4.4",
    "npm-asset/fontawesome-iconpicker":"^1.3",
    "npm-asset/slideout": "1.0.1",
    "npm-asset/shariff": "2.0.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": false,
  "scripts": {
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "extra": {
    "enable-patching": true,
    "patches": {
      "drupal/color_field": {
        "Palette instead of default_colors": "https://www.drupal.org/files/issues/default_colors_box-2807783-1.patch"
      },
      "drupal/rules": {
        "Type definition error": "https://www.drupal.org/files/issues/rules-Fix_type_definition_error-2694685-30.patch",
        "TokenProcessor Array to sting warning": "https://www.drupal.org/files/issues/rules-user_has_role-2824360-2-8x3x.patch",
        "Action send mail, implode error": "https://www.drupal.org/files/issues/system-send-email-action-fails-if-to-is-a-string-2827183-2.patch",
        "Send mail text field 2 textarea": "https://www.drupal.org/files/issues/453.patch",
        "Implement missing PluginDefinitionInterface methods in RulesUiDefinition": "https://www.drupal.org/files/issues/rules-fix-tests.patch"
      }
    }
  }
}
